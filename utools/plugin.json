        "hash shard",
        "哈希",
        "分片",
        {
          "type": "regex",
          "label": "对该内容进行哈希分片",
          "match": "/[\\da-f]+/"
        },
        {
          "type": "over",
          "label": "对该内容进行哈希分片"
        }
      ]
    }
  ]
