{
  "pluginName": "Local Server",
  "description": "使用 Http 协议创建本地静态资源服务器",
  "version": "1.0.0",
  "author": "paso",
  "main": "dist/index.html",
  "preload": "preload.js",
  "logo": "logo.svg",
  "development": {
    "main": "http://localhost:10987"
  },
